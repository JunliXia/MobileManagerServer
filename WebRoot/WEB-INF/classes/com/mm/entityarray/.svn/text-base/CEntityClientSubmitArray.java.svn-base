package com.mm.entityarray;

import java.util.ArrayList;
import java.util.List;

import com.mm.entity.CEntityClientSubmit;

public class CEntityClientSubmitArray {
	private List<CEntityClientSubmit> cEntityClientSubmitArrays=new ArrayList<CEntityClientSubmit>();

	public CEntityClientSubmitArray(
			List<CEntityClientSubmit> cEntityClientSubmitArrays) {
		this.cEntityClientSubmitArrays = cEntityClientSubmitArrays;
	}
	
	public int getSize(){
		return cEntityClientSubmitArrays.size();
	}
	
	public CEntityClientSubmit getItem(int index){
		return cEntityClientSubmitArrays.get(index);
	}

	@Override
	public String toString() {
		return "CEntityClientSubmitArray [cEntityClientSubmitArrays="
				+ cEntityClientSubmitArrays + "]";
	}
	
	
}
