package com.mm.entityarray;

import java.util.ArrayList;
import java.util.List;

import com.mm.entity.CEntityMissionDelay;

public class CEntityMissionDelayArray {

	private List<CEntityMissionDelay> cEntityMissionDelays=new ArrayList<CEntityMissionDelay>();

	public CEntityMissionDelayArray(
			List<CEntityMissionDelay> cEntityMissionDelays) {
	
		this.cEntityMissionDelays = cEntityMissionDelays;
	}

	public int getSize(){
		return cEntityMissionDelays.size();
	}
	
	public CEntityMissionDelay getItem(int index){
		return cEntityMissionDelays.get(index);
	}
	
	@Override
	public String toString() {
		return "CEntityMissionDelayArray [cEntityMissionDelays="
				+ cEntityMissionDelays + "]";
	}
	
	
	
}
