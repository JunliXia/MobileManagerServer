package com.mm.entityarray;

import java.util.ArrayList;
import java.util.List;

import com.mm.entity.CEntityVisitDelay;

public class CEntityVisitDealyArray {
	private List<CEntityVisitDelay> cEntityVisitDelays=new ArrayList<CEntityVisitDelay>();

	public CEntityVisitDealyArray(List<CEntityVisitDelay> cEntityVisitDelays) {
	
		this.cEntityVisitDelays = cEntityVisitDelays;
	}

	public int getSize(){
		return cEntityVisitDelays.size();
	}
	
	public CEntityVisitDelay getItem(int index){
		return cEntityVisitDelays.get(index);
	}
	
	@Override
	public String toString() {
		return "CEntityVisitDealyArray [cEntityVisitDelays="
				+ cEntityVisitDelays + "]";
	}
	
	
	
}
